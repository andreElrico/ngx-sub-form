    "@angular/router": "7.2.3",
    "@types/lodash-es": "4.17.3",
    "@types/uuid": "3.4.4",
    "core-js": "2.6.3",
    "lodash-es": "4.17.11",
    "rxjs": "6.4.0",
    "uuid": "3.3.2",
    "zone.js": "0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.13.0",
